<view><swiper class="screen-swiper" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="5000" duration="1000"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><block wx:if="{{item.type=='image'}}"><image style="width:100%;height:100%;" src="{{item.url}}" mode="aspectFill"></image></block><block wx:if="{{item.type=='video'}}"><video src="{{item.url}}" autoplay loop muted="{{true}}" show-play-btn="{{false}}" controls="{{false}}" objectFit="cover"></video></block></swiper-item></block></swiper><view class="myStockList"><view class="uni-flex uni-column"><view class="uni-flex uni-row" style="height:90rpx;text-align:left;line-height:80rpx;font-size:35rpx;"><text style="margin-left:50rpx;">我的自选</text><text style="margin-left:100rpx;">现价</text><text style="margin-left:100rpx;">涨跌</text><text style="margin-left:80rpx;">涨幅</text></view><image style="width:100%;height:4rpx;margin-top:-15rpx;" src="../../static/yang.png">-----------white----------</image><scroll-view class="{{['page',modalName!=null?'show':'']}}" scroll-y="{{modalName==null}}"><view class="cu-list menu-avatar"><block wx:for="{{dataArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item',modalName=='move-box-'+index?'move-cur':'']}}" data-target="{{'move-box-'+index}}" data-event-opts="{{[['touchstart',[['touchStart',['$event']]]],['touchmove',[['touchMove',['$event']]]],['touchend',[['touchEnd',['$event']]]]]}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view data-event-opts="{{[['tap',[['joinStockDetail',['$0'],[[['dataArr','',index,'code']]]]]]]}}" class="content" bindtap="__e"><view class="text-grey" style="font-size:35rpx;">{{item.stockname}}</view><view class="text-gray text-sm" style="margin-top:-20rpx;"><text class="cuIcon-infofill text-red  margin-right-xs"></text>{{''+item.code+''}}</view></view><view data-event-opts="{{[['tap',[['joinStockDetail',['$0'],[[['dataArr','',index,'code']]]]]]]}}" class="textInfo1" style="{{(item.xianjia<item.jinkai?'color:green;':'color:red;')}}" bindtap="__e">{{item.xianjia}}</view><view data-event-opts="{{[['tap',[['joinStockDetail',['$0'],[[['dataArr','',index,'code']]]]]]]}}" class="textInfo2" style="{{(item.xianjia<item.jinkai?'color:green;':'color:red;')}}" bindtap="__e">{{item.zhangdie}}</view><view data-event-opts="{{[['tap',[['joinStockDetail',['$0'],[[['dataArr','',index,'code']]]]]]]}}" class="textInfo3" style="{{(item.xianjia<item.jinkai?'color:green;':'color:red;')}}" bindtap="__e">{{item.zhangfu+'%'}}</view><view class="move"><view data-event-opts="{{[['tap',[['zhiDin',['$0'],[[['dataArr','',index,'code']]]]]]]}}" class="bg-grey" style="background-color:#BBBBBB;" bindtap="__e"><text style="margin-top:35rpx;font-size:30rpx;">置顶</text></view><view data-event-opts="{{[['tap',[['deleteArr',['$0'],[[['dataArr','',index,'id']]]]]]]}}" class="bg-red" style="background-color:#DD524D;" bindtap="__e"><text style="margin-top:35rpx;font-size:30rpx;">删除</text></view></view></view></block></view><button data-event-opts="{{[['tap',[['addStock',['$event']]]]]}}" style="width:100%;height:100rpx;background-color:#FFFFFF;" bindtap="__e">+添加股票</button></scroll-view></view></view></view>